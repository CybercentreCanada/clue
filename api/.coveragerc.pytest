[run]
branch = True
sigterm = True
data_file = .coverage.pytest
omit =
    clue/gunicorn_config.py
    clue/external/*
    clue/helper/ws.py
    clue/odm/models/*

[report]
exclude_also =
    def __repr__
    if DEBUG:
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @(abc\.)?abstractmethod]
    if "pytest" in sys.modules:
    logger.
